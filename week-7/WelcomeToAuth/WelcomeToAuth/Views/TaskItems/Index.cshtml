@model IEnumerable<WelcomeToAuth.Models.TaskItem>


@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@if (Model.Count() > 0)
{ 
    foreach (var task in Model)
    {
        <div>
            @if (task.IsCompleted)
            {
                <span>X</span>
            }
            else
            {
                <span>[ ]</span>
            }
            <span>@task.Description</span>
        </div>
    }
}
else
{
    <span>nothing to do</span>
}

