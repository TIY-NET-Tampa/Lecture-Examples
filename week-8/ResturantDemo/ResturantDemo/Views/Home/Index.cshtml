@model IEnumerable<ResturantDemo.Models.Category>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Welcome to TIY Bistro</h1>
</div>


@foreach (var cat in Model)
{
    <div class="row">
        <div class="col-md-12">
            <h2>@cat.Name</h2>
            <ul>
                @foreach(var food in cat.Menu)
                {
                    @Html.Partial("_MenuItem", food)
                }
            </ul>
        </div>
    </div>
}